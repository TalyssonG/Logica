algoritmo "semnome"

var
   cont,n,s:inteiro
inicio
   cont <-1
   s<-0
   enquanto (cont <= 5) faca
            escreva("Digite um valor: ")
              leia(n)
              s <- s + n
              cont <- cont +1
              
   fimenquanto
   escreval(" A soma de todas os valores foi ", s)
              
fimalgoritmo
